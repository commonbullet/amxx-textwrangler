		<string>this</string>
		<string>throw</string>
		<string>true</string>
		<string>try</string>
		<string>typedef</string>
		<string>typeid</string>
		<string>typename</string>
		<string>union</string>
		<string>unsigned</string>
		<string>void</string>
		<string>while</string>
		<string>xor</string>
		<string>xor_eq</string>
	</array>
	<key>BBLMLanguageCode</key>
	<string>SMA</string>
	<key>BBLMLanguageDisplayName</key>
	<string>Pawn - Amxx</string>
	<key>BBLMScansFunctions</key>
	<true/>
	<key>BBLMSupportsTextCompletion</key>
	<true/>
	<key>Language Features</key>
	<dict>
		<key>Open Strings 1</key>
		<string>&quot;</string>
		<key>Close Strings 1</key>
		<string>&quot;</string>
		<key>Open Strings 2</key>
		<string>&apos;</string>
		<key>Close Strings 2</key>
		<string>&apos;</string>
		<key>Open Block Comments</key>
		<string>/*</string>
		<key>Close Block Comments</key>
		<string>*/</string>
		<key>Comment Pattern</key>
		<string>(?x:
			(?&gt;	//		.*			$				) |
			(?&gt;	/\*		(?s:.*?)	(?: \*/ | \z )	)
		)</string>
		<key>Function Pattern</key>
		<string>(?x:
			(?P&lt;function&gt;
			
				(?P&lt;function_name&gt;
					(?&gt; _* [A-Za-z] [A-Za-z0-9_]* )
					(?:
						(?:
							(?&gt;
								(?&gt; \s+ ) | (?P&gt;comment) | (?P&gt;string)
							)
						)*
						::
						(?:
							(?&gt;
								(?&gt; \s+ ) | (?P&gt;comment) | (?P&gt;string)
							)
						)*
						~?	_* [A-Za-z] [A-Za-z0-9_]*
					)?
				)
				
				(?:
					(?&gt;
						(?&gt; \s+ ) | (?P&gt;comment) | (?P&gt;string)
					)
				)*
				
				(?P&lt;parens&gt;
					\(
						(?:
							(?&gt;
								(?&gt; [^&apos;&quot;()]+ ) | (?: / (?![/*]) ) | (?P&gt;comment) | (?P&gt;string) | (?P&gt;parens)
							)
						)*
					\)
				)
				
				(?:
					(?&gt;
						(?&gt; \s+ ) | (?P&gt;comment) | (?P&gt;string)
					)
				)*
				
				(?:
					:
					(?:
						(?&gt;
							(?&gt; [^&apos;&quot;{]+ ) | (?: / (?![/*]) ) | (?P&gt;comment) | (?P&gt;string)
						)
					)*
				)?
				
				(?P&lt;braces&gt;
					{
						(?:
							(?&gt;
								(?&gt; [^&apos;&quot;{}]+ ) | (?: / (?![/*]) ) | (?P&gt;comment) | (?P&gt;string) | (?P&gt;braces)
							)
						)*
					}
				)
			)
		)</string>
		<key>Identifier and Keyword Character Class</key>
		<string>0-9A-Z_a-z</string>
		<key>Skip Pattern</key>
		<string>(?x:
			(?&gt;
				(?P&gt;comment) | (?P&gt;string)
			)
		)</string>
		<key>String Pattern</key>
		<string>(?x:
			(?&gt;	&quot;	(?s: \\. | [^&quot;] )*?		(?: &quot; | $)	)	|
			(?&gt;	&apos;	(?s: \\. | [^&apos;] )*?		(?: &apos; | $)	)
		)</string>
		<key>Escape Char in Strings 1</key>
		<string>^</string>
		<key>Escape Char in Strings 2</key>
		<string>^</string>
		<key>End-of-line Ends Strings 1</key>
		<true/>
		<key>Terminator for Prototypes 1</key>
		<string>;</string>
	</dict>
</dict>
</plist>
